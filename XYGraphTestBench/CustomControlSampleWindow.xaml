<WpfControlTestbenchLib:TestbenchWindow x:Class="XYGraphLib.CustomControlSampleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:WpfControlTestbenchLib="clr-namespace:WpfTestbench;assembly=WpfControlTestbenchLib"
        xmlns:XYGraphTracedLib="clr-namespace:XYGraphLib;assembly=XYGraphTracedLib"
                     
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        mc:Ignorable="d" 
        d:DesignHeight="300" d:DesignWidth="1000"
        Title="Custom Control Sample">
  <WpfControlTestbenchLib:WpfControlTestbench>
    <WpfControlTestbenchLib:WpfControlTestbench.TestFrameworkElement>
      <XYGraphTracedLib:CustomControlSampleTraced Name="TestCustomControlSampleTraced"/>
    </WpfControlTestbenchLib:WpfControlTestbench.TestFrameworkElement>
  </WpfControlTestbenchLib:WpfControlTestbench>
  
  <!--Put textboxes, etc. for testing control specific properties here-->
</WpfControlTestbenchLib:TestbenchWindow>
